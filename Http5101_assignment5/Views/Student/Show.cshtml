
@model Http5101_assignment5.Models.Student

@{
    ViewBag.Title = "Show";
}


<div id="containerbox">

    <nav>
        <a class="navlink" href="/Student/List">Go Back To Students</a>
    </nav>
</div>

<div>

    <h2>@Model.StudentFname @Model.StudentLname</h2>
    <table class="table table-sm">
        <tbody>
            <tr>
                <th scope="row">Student Number</th>
                <td>@Model.StudentNumber</td>
            </tr>
            <tr>
                <th scope="row">Enrol Date</th>
                <td>@Model.EnrolDate</td>

            </tr>

        </tbody>
    </table>



</div>


