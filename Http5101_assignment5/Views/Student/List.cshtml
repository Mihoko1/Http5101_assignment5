
@model IEnumerable<Http5101_assignment5.Models.Student>

@{
    ViewBag.Title = "List";
}
<div id="containerbox">

    <div class="align">
        <h2>Student List</h2>
    </div>

    <div class="list">

        @foreach (var student in Model)
        {
            <div>

                <a href="/Student/Show/@student.StudentId"><span class="marginRight15">@student.StudentNumber</span> @student.StudentFname @student.StudentLname</a>

            </div>

        }

    </div>

</div>


