@model IEnumerable<Http5101_assignment5.Models.Class>

@{
    ViewBag.Title = "List";
}
<div id="containerbox">

    <div class="align">
        <h2>Class List</h2>
    </div>

    <div class="list">

        @foreach (var classes in Model)
        {
        <div>

            <a href="/Class/Show/@classes.ClassId"><span class="marginRight15">@classes.ClassCode</span>@classes.ClassName </a>

        </div>

        }

    </div>

</div>



